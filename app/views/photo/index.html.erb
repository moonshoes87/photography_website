<div id="display">
  <p>Put a default pretty image here</p>
</div>
<div id="slider">
  <div id="click-back">Click back</div>
  <ul id="thumbnails">
    <% @photos.each do |p|  %>
    <li class="nail"><%= image_tag "#{p.url}", class: "thumb crop portrait" %></li>
    <% end %>
  </ul>
  <div id="click-forward">Click left</div>
</div>

